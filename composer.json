{
    "name": "railken/lara-ore-auth",
    "keywords": ["laravel", "lara-ore", "lara-ore-auth", "auth"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "laravel/passport": "^5.0",
        "railken/lara-ore-api": "dev-master",
        "railken/lara-ore-support": "dev-master",
        "railken/lara-ore-user": "dev-master"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.9.2",
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "~3.6",
        "friendsofphp/php-cs-fixer": "^2.8.1"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\LaraOre\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Railken\\LaraOre\\Auth\\Tests\\": "tests/" }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Railken\\LaraOre\\AuthServiceProvider"
            ]
        }
    }
}
