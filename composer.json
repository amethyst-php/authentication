{
    "name": "railken/amethyst-authentication",
    "keywords": ["laravel", "amethyst", "amethyst-authentication", "auth"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "laravel/passport": "^6.0",
        "railken/amethyst-common": "0.1.*",
        "railken/amethyst-user": "0.1.*",
        "laravel/socialite": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "3.6.*",
        "duncan3dc/dusk": "^0.8.0",
        "donatj/mock-webserver": "^2.0"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Railken\\Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Railken\\Amethyst\\Providers\\AuthenticationServiceProvider"
            ]
        }
    }
}
